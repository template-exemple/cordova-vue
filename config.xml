<?xml version='1.0' encoding='utf-8'?>
<widget id="com.pierregauthier.cordova-vue-template" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Cordova-Vue-Template</name>
    <description>
        Cordova Vue Webpack Tempate.
    </description>
    <author email="p_gauthier@live.fr">
        Pierre Gauthier
    </author>
    <content src="index.html" />
    <icon src="www/media/skeleton.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="org.awokenwell.proximity" spec="https://github.com/awoken-well/cordova-plugin-proximity.git" />
</widget>
